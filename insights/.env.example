# Azure Application Insights Configuration
# Copy this file to .env and fill in your actual values

# Your Application Insights Resource ID
# Format: /subscriptions/{subscription-id}/resourcegroups/{resource-group}/providers/microsoft.insights/components/{app-insights-name}
AZURE_APP_ID=your_application_insights_id_here

# Azure AD Application (Client) ID
AZURE_CLIENT_ID=your_client_id_here

# Azure AD Application Client Secret
AZURE_CLIENT_SECRET=your_client_secret_here

# Azure AD Tenant ID
AZURE_TENANT_ID=your_tenant_id_here
